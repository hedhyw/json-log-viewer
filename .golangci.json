{
  "formatters": {
    "enable": [
      "gofmt",
      "gofumpt",
      "goimports"
    ],
    "exclusions": {
      "generated": "lax",
      "paths": [
        "third_party$",
        "builtin$",
        "examples$"
      ]
    },
    "settings": {
      "goimports": {
        "local-prefixes": [
          "github.com/hedhyw/json-log-viewer/"
        ]
      }
    }
  },
  "linters": {
    "default": "all",
    "disable": [
      "bodyclose",
      "depguard",
      "exhaustive",
      "exhaustruct",
      "funlen",
      "gochecknoglobals",
      "gomoddirectives",
      "ireturn",
      "lll",
      "mnd",
      "nlreturn",
      "nolintlint",
      "nonamedreturns",
      "paralleltest",
      "rowserrcheck",
      "tagalign",
      "tagliatelle",
      "testpackage",
      "thelper",
      "varnamelen",
      "wastedassign",
      "wrapcheck",
      "wsl"
    ],
    "exclusions": {
      "generated": "lax",
      "paths": [
        "third_party$",
        "builtin$",
        "examples$"
      ],
      "presets": [
        "comments",
        "common-false-positives",
        "legacy",
        "std-error-handling"
      ]
    },
    "settings": {
      "cyclop": {
        "max-complexity": 15
      }
    }
  },
  "version": "2"
}
