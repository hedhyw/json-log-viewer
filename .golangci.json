    "disable": [
      "lll",
      "gochecknoglobals",
      "bodyclose",
      "wsl",
      "funlen",
      "maligned",
      "exhaustivestruct",
      "gci",
      "wrapcheck",
      "varnamelen",
      "testpackage",
      "exhaustive",
      "gomnd",
      "thelper",
      "paralleltest",
      "tagliatelle",
      "scopelint",
      "golint",
      "interfacer",
      "nonamedreturns",
      "exhaustruct",
      "nolintlint",
      "deadcode",
      "wastedassign",
      "structcheck",
      "varcheck",
      "ifshort",
      "nosnakecase",
      "rowserrcheck",
